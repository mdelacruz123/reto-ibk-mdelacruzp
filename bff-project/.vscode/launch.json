{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/nest",
        "args": ["start"],
        "envFile": "${workspaceFolder}/.env",
        "env": {
          "NODE_ENV": "development"
        },
        "sourceMaps": true
      }
    ]
  }